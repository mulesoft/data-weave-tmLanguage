{
  "name": "tests",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "test.ts",
  "scripts": {
    "updateSnapshot": "npm run updateFullSnapshot && npm run updateSimpleSnapshot",
    "updateFullSnapshot": "vscode-tmgrammar-snap -s source.data-weave -g ../DataWeave.tmLanguage 'cases/*.dwl' --updateSnapshot",
    "updateSimpleSnapshot": "vscode-tmgrammar-snap -s source.data-weave -g ../DataWeave-simple.tmLanguage 'cases_simple/*.dwl' --updateSnapshot",
    "testFullGrammar": "vscode-tmgrammar-snap -s source.data-weave -g ../DataWeave.tmLanguage 'cases/*.dwl'",
    "testSimpleGrammar": "vscode-tmgrammar-snap -s source.data-weave -g ../DataWeave-simple.tmLanguage 'cases_simple/*.dwl'",
    "test": "npm run testFullGrammar && npm run testSimpleGrammar"
  },
  "dependencies": {
    "vscode-tmgrammar-test": "latest"
  }
}
